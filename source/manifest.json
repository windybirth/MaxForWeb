{
  "name": "Max for web",
  "version": "1.0",
  "description": "DotaMax on chrome (Personal development). Can read max news and bbs.",
  "permissions": ["storage"],
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon_default_180x.png"
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com https://*.bootstrapcdn.com https://cdnjs.cloudflare.com; object-src 'self'"
}
